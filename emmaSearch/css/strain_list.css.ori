	font-weight: normal;
	color: #104E8B;
}
p.mutype_info {
	text-align: center;
	padding: 10px;
}
p.mutype_info a {
	color: #104E8B;
}
div.letters a.homeicon img, div#strain_data a.homeicon img, .ui-tabs-panel a.homeicon img {
	display: inline;
