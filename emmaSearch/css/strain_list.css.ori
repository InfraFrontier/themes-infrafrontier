    padding: 5px;   
    color: white;
}
div.strainType a {
    color: white;
}
/***** strain menu ******/
div#strain_menu {
    padding: 0;
    margin-left: -20px;
}
ul.menu {    
    width: 95%;
    font-size: 12px;
}
ul.menu a {
    color: white;
    text-decoration: none;
    /*display: block;*/
    /*width: 100%;*/
}
ul.menu li {
    position: relative;
    padding: 5px 10px;
    display: inline;
}
ul.menu li > a {
    color: black;
}
ul.menu ul { 
    background-color: #EAEAEA; /*: url("../images/stooltip.png") repeat scroll 0 0 transparent;*/
    border: 2px double white;
    z-index: 9999; 
    /*list-style: none;*/
    padding: 10px;
    margin: 0; 
    width: auto;
    position: absolute;
    top: 22px;
    left: 10px;
    /*visibility: hidden;*/
    display: none;    
}
ul.sublist {
    width: 200px !important;
    border: none !important;
}
ul.menu > li:hover > ul {
  /*  display: block; */
}
li.menu > ul > li:hover {
   background-color: #606060;  
}
ul.sublist li:hover { 
    background: #606060 url("../images/right_arrow_b_on_w.png") no-repeat 2px center;
}
ul.sublist li:hover a, li.menu > ul > li:hover > a {
       color: white;     
}
ul.sublist li {
    background: #EAEAEA url("../images/right_arrow_w_on_b.png") no-repeat 2px center;
    /*background:url("../images/right_arrow_w_on_b.png") 0px 4px no-repeat; */
}
ul.menu > li > ul > li {
    /*visibility: visible;*/
    display: block; 
    padding-left: 25px;
}
li.singleton {
    background: #EAEAEA url("../images/right_arrow_w_on_b.png") no-repeat 2px center;
}
li.singleton:hover {
    background: #EAEAEA url("../images/right_arrow_b_on_w.png") no-repeat 2px center;
}
li.liMenu {
    padding-left: 0 !important;
    width: 280px !important;
}
li.menu > ul > li > ul {
   /* visibility: visible;*/  
    position: relative;
    display: block; 
    margin: -28px 0 25px 0;
    padding-left: 5px;
}
li.menu > ul li, li.menu > ul li a{
    font-size: 11px;
    margin: 0;
    padding: 0;   
    display: block;
}
span.ulOpen, span.submenu {
    background: #EAEAEA url("../images/minus_s.png") no-repeat 2px center;
    padding-left: 25px !important; 
}
span.ulClose {
    background: #EAEAEA url("../images/plus_s.png") no-repeat 2px center;
    padding-left: 25px !important; 
}
span.submenu {
    font-weight: bold;
    cursor: pointer;    
}
ul.sublist, li.liMenu {
    /*border: none !important; */
    background-color:#EAEAEA !important;      
}
ul.sublist {
    margin-top: -20px !important;
}
ul.menu ul li {
    padding: 0 5px 3px 35px;
    width: 220px;
}

/***** end of strain menu ******/

