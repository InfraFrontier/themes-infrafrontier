/* strain list page */
div.helpBox {
    margin-top: 15px;
}
a.icon.help {
    background: url("../images/help.png") no-repeat scroll left center transparent;
    color: #909090;
    padding: 0 18px;
    text-decoration: none;
}
div#srchBlkC1 {
    position: relative;
}
div#srchTooltip {
    display: none;
    background: url("../images/stooltip_help.png") repeat scroll 0 0 transparent;
    display: none;
    height: 262px;
    line-height: 1.1em;
    margin: 0;
    padding: 20px ;
    position: absolute;
    top: -8px;
    left: 45px;
    text-align: right;
    width: 240px;
    z-index: 999;  
    font-family: 'Open Sans',Arial,Helvetica,sans-serif; 
    line-height: 1.2em;
    outline: 0 none;  
    vertical-align: baseline;    
}
div#srchTooltip h6 {
    font-family: 'Open Sans',Arial,Helvetica,sans-serif;
    font-size: 14px;  
    line-height: 14px;
}
div#srchTooltip ul {
    margin-right: 10px;
}
div#srchTooltip li {
    font-size: 12px;
    font-weight: bold;
    line-height: 13.2px;
    margin: 5px;   
    list-style-type: disc;
    list-style-position: outside;
    direction: rtl;    
}
div#srchTooltip li span {
    font-style: italic;
    font-weight: normal;
    font-size: 10px;
    line-height: 13.2px;
}
div#srchTooltip p {
    font-family: inherit;
    font-size: 10px;
    line-height: 13.2px;
}
div#browse_by_strains, div#browse_by_genes, div#browse_by_human_diseases,


div[id^=ui-tabs-] {
	margin:0;
	padding: 0;
	width: auto;
	height: auto;
}
p.strainCategory {
	color: black;
	font-size: 12px;
	font-weight: bold;
	text-align: left;
}
li.strainLink {
	cursor: pointer;
	font-size: 10px;
	padding: 5px;
	
}
div#strain_data {
	margin: 15px auto;
	width: auto;
	clear: both; 
}
div#gene_data, div#omim_emma {
	margin: 0 auto 20px auto;
	width: auto; 
}
div#strainInfo {
	border: none;
	overflow: auto;
	width: auto;
}
li.omd {
	margin-left: 10px;	
	padding: 2px;
}	
div.letters  {
	margin: 0;
	padding: 0 20px 10px 12px;
}
a.ltrLink, a.omLtrLink {
	padding: 5px;
	color: #104E8B;
	font-weight: bold;
}
p#errmsg {
	margin-left: 0px;
}
div.loader {
	text-align: center;
	margin-top: 100px;
}
div.loader {
	position: absolute;
	top: 150px;
	left: 50%;	
}
div.loader span {
	margin: -5px 0 0 5px;
}
span.killTab{
	right: 0px;
	margin-top: 5px;
	color: #8B0000;
	font-weight: bolder;
