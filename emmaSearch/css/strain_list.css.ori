	color: white;
	margin: 5px 0;;
}
#desc_general {
	margin: 5px;
	padding: 2px;
}

span.qc {	
	padding-right: 10px;
	cursor: pointer;
	color: #104E8B;
	font-weight: bold;
}
a#qcKB {padding: 0 10px; font-size: 10px; float: right; font-weight: normal !important; text-decoration: none; border: 1px solid gray;
padding: 2px 3px; background-color: #104E8B; color: white !important;}	
div#qcData {margin-top: 3px; display: none;}
table.qc {
	width: 100%;
	border-collapse: collapse;	 
	margin: 3px 0 10px 0;    
}

table.qc tr td { 
    color: black; 
    border: 1px solid #CCCCCC !important;
    background-color: none !important;
}
div.qcCap {font-weight: bold; border-left: 10px solid #2F4F4F; padding-left: 5px;}
table.qc td.qcCol {
    font-size: 10px;  
}
div.qc_block {
    margin: 10px 0;
	padding: 3px 5px; 
    color: black;
	background-color: #CCCCCC;
}

div#strain_list_container {
    background-color: #EFEFEF;
}
div#search_bar, div#emma_wrapper {
	width: 80%;
    padding: 0;
	margin: 10px auto;
}

ul.order {
    list-style: none;
    padding:0;
    margin:0;
}
ul.order li { 
    padding-left: 1em; 
    text-indent: -.7em;
}
ul.order li#greenDot {
    background: url("../images/green_dot.png") no-repeat 0 center;  
    padding: 0 0 0 25px;
}
ul.order li#orangeDot {
    background: url("../images/orange_dot.png") no-repeat 0 center;  
    padding: 0 0 0 25px;
}
ul.order li#redDot {
    background: url("../images/red_dot.png") no-repeat 0 center;  
    padding: 0 0 0 25px;
}
div#strainTabs {
    border: none;
    margin: 0 auto !important;   
    padding: 0 !important;
}
ul.ui-tabs-nav {
    padding: 0 !important;
    background-color: #EFEFEF !important;
}
li.ui-state-active {
    border: 1px solid white !important;
}
li.ui-state-active a {
    background-color: white !important;
    font-size: 12px !important;
    font-weight: bold;
}
ul.ui-tabs-nav li a {
     font-size: 12px !important;
}
div#searchHint {   
    font-size: 10px; 
    color: gray;
    font-style: italic;
}
input.loupe {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: url("../images/loupe.png") no-repeat scroll center center #FFFFFF;
    border-color: -moz-use-text-color -moz-use-text-color -moz-use-text-color #C0C0C0;
    border-image: none;
    border-radius: 0 0 0 0;
    border-style: none none none solid;
    border-width: medium medium medium 1px;
    height: 20px;
    margin: 4px 0 0 -29px;
    position: absolute;
    width: 23px;
    z-index: 600;
}
table.dataTable {
    font-size: 11px !important;  
    width: 100%;  
    text-align: left;
}
table.dataTable tr:hover td {
   /* background-color: #EBECE4;*/
}
table.dataTable td {
    background-color: #F4F4F4;
    border: 1px solid #CCCCCC;
    font-weight: normal;  
}
table.dataTable td.undefined { 
    background-color: white;
}
tr.selectedTr td {
    background-color: #606060;
    color: white;
}
tr.selectedTr td a {
    color: white !important;
}
input#searchInputBox {
    width: 100% !important;
    height: 18px !important;
}
tr.hiddenRow {display: none;}
div.dataTables_wrapper {margin: 30px 20px 50px 20px !important;}

td.toggle, td.order {
    text-align: center !important;
}
table.desc {   
    width: 95%; 
    margin: 25px; 
    text-align: left !important;
    font-weight: normal;
}
table.desc tr {
    border: none !important;    
}
table.desc td {
    background-color: white;
    border-left: none !important;
    border-right: none !important;
}
table.desc td:nth-child(1) {
    padding-left: 20px;
    min-width: 25% !important;
}
td.infoType div {    
    text-align: left !important;     
    font-size: 12px;
    font-weight: bold;
}
tr.providerSec td, tr.emmaSec td {   
    background-color: #CCCCCC !important;    
    font-size: 12px; 
}
tr.providerSec td.selected, tr.emmaSec td.selected {  
    background-color: #606060;   
}
tr.providerSec td:nth-child(2), tr.emmaSec td:nth-child(2)  {
    text-align: right !important;
    padding-right: 20px !important;
} 
div.descAccordion {
    width: 95%;
    margin: 25px;
}
div.descAccordion h3 {
    background-image: none !important;
    background-color: #CCCCCC !important;
    padding: 5px 5px 5px 15px !important;
    height: 20px;
    text-align: left;
    font-size: 12px;
    font-weight: bold;
}
div.descAccordion h3.ui-state-default {
    background-color: #CCCCCC !important;   
}
div.descAccordion h3.ui-state-active {
    background-color: #606060 !important;      
     color: white !important;
}
div.descAccordion h3.ui-state-default span.ui-icon {
    left: 97% !important;
    margin-top: -10px !important;
    width: 20px;
    height: 20px;  
    background: url("../images/plus.png") no-repeat center center #CCCCCC;
}
div.descAccordion h3.ui-state-active span.ui-icon {      
    left: 97% !important;
    margin-top: -10px !important;
    width: 20px;
    height: 20px;  
    background: none; /*url("../images/minus.png") no-repeat center center #606060;*/
}
div.descActionRow {
    width: 95%;
    margin: 25px auto !important;
}
div.descActionRow span {
    border: none;
    background-color: #606060;
    padding: 3px 10px;
    margin-right: 5px;
    color: white;
    font-size: 11px;
    cursor: pointer;    
}
div.descActionRow span a {
    text-decoration: none;
    color: white;
}
div.descActionRow span.grayout, div.descActionRow span.grayout a {
    background-color: #F4F4F4;
    color: gray;
    cursor: text !important;
}
span.mgiLink {
    position: relative;
}
span.mgiLink div {
    display: none;
    position: absolute;
    min-width: 120px;
    top: -110px;
    left: 0px;
    border: 1px solid black;
    padding: 5px;
    z-index: 999;
    color: black;
    background-color: white;    
}
span.mgiLink div a {
    color: #104E8B; 
}

/* override jquery ui tabs */
ul.ui-tabs-nav { 
    padding: 0px; 
    background: none; 
    border-width: 0px; 
} 
ul.ui-tabs-nav { 
    padding-left: 0px; 
    background: transparent; 
    border-width: 0px 0px 1px 0px; 
    -moz-border-radius: 0px; 
    -webkit-border-radius: 0px; 
    border-radius: 0px; 
} 
ul.ui-tabs-panel { 
    background: #f5f3e5 url(http://code.jquery.com/ui/1.8.23/themes/south-street/images/ui-bg_highlight-hard_100_f5f3e5_1x100.png) repeat-x scroll 50% top; 
    border-width: 0px 1px 1px 1px; 
}
/* letter blocks */
